{
	"private": true,
	"license": "MIT",
	"author": "Joel Gonzales",
	"scripts": {
		"dev:watch": "cross-env DEBUG=assistance-service:* NODE_ENV=development nodemon ./server.js --exec 'babel-node'",
		"dev": "npm run dev:watch",
		"dev:inspect": "npm run dev:watch -- --inspect",
		"lint": "eslint --format './node_modules/eslint-friendly-formatter/index.js' src/index.js",
		"lint:watch": "cross-env NODE_ENV=development esw --clear --watch --format './node_modules/eslint-friendly-formatter/index.js' src/index.js",
		"test": "cross-env NODE_ENV=development jest --config=jest.config.json",
		"test:watch": "npm run test -- --watch",
		"test:coverage": "npm run test -- --coverage",
		"migrate": "NODE_ENV=development knex migrate:latest",
		"rollback": "NODE_ENV=development knex migrate:rollback",
		"migrate-prod": "NODE_ENV=production knex migrate:latest",
		"rollback-prod": "NODE_ENV=production knex migrate:rollback",
		"seed:run": "NODE_ENV=development knex seed:run",
		"seed:run-prod": "NODE_ENV=production knex seed:run",
		"commit": "committer",
		"public": "shx rm -rf public && shx mkdir public",
		"dist": "shx rm -rf dist && shx mkdir dist",
		"build": "cross-env NODE_ENV=production babel src -d dist",
		"start": "DEBUG=assistance-service:* NODE_ENV=production node server.js",
		"production": "pm2 start server.js -n web-service",
		"deploy:production": "fly production"
	},
